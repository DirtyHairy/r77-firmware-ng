\&        [, [[, add\-shell, ash, awk, base64, basename, blkid, blockdev,
\&        bunzip2, bzcat, bzip2, cal, cat, catv, chattr, chgrp, chmod, chown,
\&        chroot, chrt, cksum, cmp, comm, cp, cttyhack, cut, date, dd, depmod,
\&        devmem, df, dhcprelay, dirname, dmesg, dnsdomainname, dos2unix, du,
\&        dumpleases, echo, ed, egrep, eject, env, expand, expr, false, fdisk,
\&        fgrep, find, flock, fold, free, fsck, fstrim, fsync, ftpget, ftpput,
\&        grep, groups, gunzip, gzip, halt, hd, head, hexdump, hostid,
\&        hostname, hwclock, id, ifconfig, ifdown, ifup, init, insmod,
\&        install, iostat, ip, ipaddr, iplink, iproute, iprule, iptunnel,
\&        kill, killall, killall5, ln, logname, losetup, ls, lsattr, lsmod,
\&        lsof, lsusb, lzop, lzopcat, md5sum, mdev, mesg, mkdir, mkdosfs,
